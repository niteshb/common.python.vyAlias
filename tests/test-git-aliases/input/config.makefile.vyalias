make
    --vyabsg-no-command--
    label: --vyabsg-null-label--
        h, --vyabsg-null-alias--, -h, --help
            --vyabsg-no-command--
            label: help
            snippet: This help message
        git
            python main.py input/config.vyalias -o output -f g.vyalias.cmd
            python main.py input/config.vyalias.py -o output -f g.py.cmd
            label: git
            snippet: <= Generate git alias files from both, vyalias & python, config files
        make
            python main.py input/config.makefile.vyalias
            snippet: <= Generates this make batch file from its config file
            label: make
        exe
            make clear
            nuitka main.py
            label: exe
            snippet: <= Generate main.exe from main.py
        clear, clean
            if exist main.exe del main.exe
            if exist main.build make __clear_main_build
            label: clear
            snippet: <= Cleans up stuff from nuitka
        __clear_main_build
            cd main.build
            del /f/q/s *
            cd ..
            rmdir /q/s main.build
            label: clear_main_build
            snippet: <= Cleans up main.build from nuitka
